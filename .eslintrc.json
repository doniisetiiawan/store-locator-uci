{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "array-callback-return": "off",
    "consistent-return": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off",
    "eqeqeq": "off",
    "no-param-reassign": "off"
  }
}
